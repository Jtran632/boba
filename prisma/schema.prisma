// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model Drink {
  id String @id @default(cuid())
  flavors String
  toppings String
  teaBase String
  sweetness Int
  ice String
  cost Decimal
}